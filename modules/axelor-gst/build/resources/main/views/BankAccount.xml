<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <selection name="bankaccount.type.selection">
    <option value="saving">saving</option>
    <option value="current">current</option>
  </selection>

  <form name="bank-account-form" title="Bank account"
    model="com.axelor.gst.db.BankAccount">
    <panel title="Bank Account Details" name="bankAccountPanel">
      <field name="bank" />
      <field name="accountNo" />
      <field name="ifsc" />
      <field name="type" />
      <field name="address" colSpan="12" />
    </panel>
  </form>

  <grid name="bank-account-grid" title="Bank accounts"
    model="com.axelor.gst.db.BankAccount">
    <field name="bank" />
    <field name="accountNo" />
    <field name="ifsc" />
    <field name="type" />
    <field name="address" />
  </grid>
</object-views>
