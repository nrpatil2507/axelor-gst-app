            <xml-property name="queryText"><![CDATA[select i.reference,
i.invoice_datet,
i.net_cgst,
i.net_sgst,
i.net_igst,
p.name,
l.igst,
l.cgst,
l.sgst,
l.price,
l.net_amount,
l.gross_amount,
l.item,l.qty,
i.net_amount as totalNetAmount,
i.gross_amount as totalgrossAmount,
a.line1,a.line2,a.pin_Code,c.name as city
from gst_invoice as i 
left join gst_party as p on p.id=i.party
left join gst_invoice_line as l on l.invoice=i.id
left join gst_address as a on a.id=i.invoice_address
left join gst_city as c on c.id=a.city
where i.id=?;
]]></xml-property>
