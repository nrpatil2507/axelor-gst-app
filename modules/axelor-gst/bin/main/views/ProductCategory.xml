<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid name="product-category-grid" title="Product categories"
    model="com.axelor.gst.db.ProductCategory" edit-icon="true">
    <field name="name" />
    <field name="code" />
    <field name="gstRate" />
  </grid>

  <form name="product-category-form" title="Product category"
    model="com.axelor.gst.db.ProductCategory">
    <panel title="Product Category" name="productCategoryPanel">
      <field name="name" colSpan="4" />
      <field name="code" colSpan="4" />
      <field name="gstRate" colSpan="4" />
    </panel>
  </form>

</object-views>
